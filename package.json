{
  "private": true,
  "version": "1.5.2",
  "author": {
    "name": "WOOKONG Team",
    "email": "developer@wooko.ng"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production electron app/dist/main",
    "postinstall": "cd app && cross-env ELECTRON_MIRROR=https://gh-contractor-zcbenz.cnpmjs.org/atom-shell/dist/ npm install && cd .. && npm run rebuild",
    "dev:renderer": "cross-env NODE_ENV=production webpack-dev-server --mode development --config webpack.config.renderer.babel.js",
    "dev:main": "webpack -w --mode development --config ./webpack.config.main.babel.js",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "clean": "chmod u+x dist && rm -rf node_modules app/node_modules dist app/dist",
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.babel.js",
    "lint": "eslint --ext .js src test webpack.config.babel.js",
    "test": "mocha test/**/*-test.js --require babel-register --require ./test/setup --no-timeouts",
    "rebuild": "electron-rebuild -d=https://gh-contractor-zcbenz.cnpmjs.org/atom-shell/dist/ -m ./app",
    "pack:mac": "cross-env ELECTRON_CONSOLE=false gulp prepare:mac && cross-env NODE_ENV=production SRC=tmp webpack --mode production --config ./webpack.config.babel.js && npm run rebuild && build -p always",
    "pack:win": "cross-env ELECTRON_CONSOLE=false gulp prepare:win && cross-env NODE_ENV=production SRC=tmp webpack --mode production --config ./webpack.config.babel.js && npm run rebuild && build -p always",
    "pack:win-dev": "cross-env ELECTRON_CONSOLE=true gulp prepare:win && cross-env NODE_ENV=production SRC=tmp webpack --mode production --config ./webpack.config.babel.js && npm run rebuild && build -p always",
    "pack:mac-dev": "cross-env ELECTRON_CONSOLE=true gulp prepare:mac && cross-env NODE_ENV=production SRC=tmp webpack --mode production --config ./webpack.config.babel.js && npm run rebuild && build -p always",
    "pack:dir": "npm run build && npm run rebuild && build --dir",
    "pack:justBuild": "build --dir",
    "precommit": "lint-staged"
  },
  "build": {
    "appId": "com.nbltrust.wookong-solo",
    "asar": false,
    "files": [
      "**/*",
      "dist/*"
    ],
    "publish": {
      "provider": "generic",
      "url": "https://nebuladownload.oss-cn-beijing.aliyuncs.com/wookong/personal/"
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        "dmg",
        "zip"
      ],
      "type": "distribution",
      "artifactName": "${name}_${version}.${ext}",
      "identity": "Yanfeng Chen (M5LYGNKFLB)"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${name}_Setup_${version}.${ext}"
    }
  },
